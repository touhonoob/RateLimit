{
    "type": "library",
    "name": "touhonoob/rate-limit",
    "description": "Rate Limiting Library With Token Bucket Algorithm",
    "version": "1.2.0",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6.6",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "predis/predis": "^1.1"
    },
    "suggest": {
        "ext-redis": "^2.2",
        "ext-apc": "^4.0",
        "ext-apcu": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "touhonoob",
            "email": "touhonoob@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Touhonoob\\RateLimit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Touhonoob\\RateLimit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "@php ./vendor/bin/parallel-lint --exclude vendor/ ."
    }
}
